{
    "name": "dflydev/stack-hawk",
    "autoload": {
        "psr-0": {
            "Dflydev\\Stack": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dflydev/dflydev-hawk"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dflydev/dflydev-stack-authentication"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dflydev/dflydev-stack-firewall"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "dflydev/hawk": "1.0.*@dev",
        "dflydev/stack-authentication": "1.0.*@dev",
        "dflydev/stack-firewall": "1.0.*@dev",
        "symfony/http-foundation": "~2.1",
        "symfony/http-kernel": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.21",
        "silex/silex": "1.1.*@dev",
        "stack/builder": "~1.0@dev",
        "stack/callable-http-kernel": "~1.0@dev",
        "stack/inline": "~1.0@dev",
        "symfony/browser-kit": "~2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
